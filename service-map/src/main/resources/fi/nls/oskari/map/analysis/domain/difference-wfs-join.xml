<wfs:GetFeature handle="GeoTools 10.2 WFS DataStore" count="{maxFeatures}" outputFormat="application/json" resultType="results" service="WFS" version="2.0.0" srsName="{srsName}" {xmlns} xmlns:wfs="http://www.opengis.net/wfs/2.0" xmlns:ogc="http://ww.opengis.org/ogc/2.0" xmlns:fes="http://www.opengis.net/fes/2.0" xmlns:gml="http://www.opengis.net/gm1/3.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/wfs/2.0 
